//prg to accept one Number and check whether it is divisible by 3 and 5 or not.
//developed by Ramendra date:11/09/2025
#include <stdio.h>
void main()
{
    int a;
    printf("Enter the number\n");
    scanf("%d",&a);
    if(a%3==0)
    {
        if(a%5==0)
        printf("Divisible by 3 and 5");
        else
        printf("Not Divisible by 3 and 5");
    }
    else
    printf("Not Divisible by 3 and 5");
}