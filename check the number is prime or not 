//program to check prime or not
// developed by Raj
#include <stdio.h>
void main()
{
    int a,c=0;
    printf("Enter the Number\n");
    scanf("%d",&a);
    for(int i=1; i<=(a/2)+1; i++)
    {
        if(a%i==0)
        c++;
    }
    if(c==1)
    printf("A Prime Number");
    else
    printf("Not A Prime Number");
}